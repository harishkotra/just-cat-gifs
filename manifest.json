{
    "manifest_version": 3,
    "name": "Just Cat GIFs",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "permissions": [
      "scripting",
      "activeTab"
    ],
    "host_permissions": [
      "https://cataas.com/*"
    ]
  }
  